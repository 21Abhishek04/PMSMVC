
@{
    ViewData["Title"] = "DoctorDashBoard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">




    <link rel="stylesheet" href="~/css/maicons.css">

    <link rel="stylesheet" href="~/css/bootstrap.css">

    <link rel="stylesheet" href="~/vendor/owl-carousel/css/owl.carousel.css">

    <link rel="stylesheet" href="~/vendor/animate/animate.css">

    <link rel="stylesheet" href="~/css/theme.css">


    <script src="~/js/jquery-3.5.1.min.js"></script>

    <script src="~/js/bootstrap.bundle.min.js"></script>

    <script src="~/vendor/owl-carousel/js/owl.carousel.min.js"></script>

    <script src="~/vendor/wow/wow.min.js"></script>

    <script src="~/js/theme.js"></script>


</head>
<header>
    <nav class="navbar navbar-expand-lg navbar-light shadow-sm">

        <a class="navbar-brand" href="#"><span class="text-primary">Life</span> Care</a>


        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupport" aria-controls="navbarSupport" aria-expanded="true" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupport">
            <ul class="navbar-nav ml-auto">

                <li class="nav-item">
                    <a class="btn btn-primary ml-lg-3" asp-action="GetPatients" asp-controller="Doctor">Get all Patients</a>
                </li>
                <li class="nav-item">
                    <a class="btn btn-primary ml-lg-3" asp-action="GetAppointment" asp-controller="Doctor">View all Appointments</a>
                </li>
                <li class="nav-item">
                    <a class="btn btn-primary ml-lg-3"  asp-controller="Home" asp-action="Index">Logout</a>
                    @*<a class="btn btn-primary ml-lg-3" asp-action="Index" asp-controller="Home">Log Out</a>*@
                </li>
            </ul>
        </div> <!-- .navbar-collapse -->

    </nav>

</header>
<h1>Doctor DashBoard</h1>
<h2>Welcome</h2>
